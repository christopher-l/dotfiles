hi Normal color: black; background: #eeeeee;
hi StatusLine color: #444444; background: transparent; font-weight: normal;
hi ContentSeparator border-top: 1px solid #a0a0a0; display: -moz-box;

"set showstatuslinks=2
"set status=input,location,history,tabcount,position
set defsearch=dd
"set gui=notabs

noremap zz :set gui=invtabs,invnavigation invsb<CR>
noremap zt :set gui=invnavigation<CR>
noremap <BS> :noh<CR>
noremap j <Down>
noremap k <Up>
noremap h <Left>
noremap l <Right>
noremap J j
noremap K k
noremap d :bd -s lastactive<CR>
noremap ,r :reloadall<CR>
